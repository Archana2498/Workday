<?xml version="1.0" encoding="UTF-8"?><database>
    <element extends="sn_hr_integr_fw_todo_inbound" label="Workday Todos" max_length="40" name="sn_hr_workday_todo" type="collection">
        <element label="Workday completed by admin" max_length="40" name="completed_by_admin_workday" read_only="true" type="boolean"/>
        <element label="Workday last functionally updated" max_length="40" name="last_functionally_updated_workday" type="glide_date_time"/>
        <element label="Workday unique id to be updated" max_length="40" name="wd_unique_id_to_be_updated" read_only="true" type="boolean"/>
        <element label="Workday completed by ID" max_length="40" name="workday_completed_by_id" type="string"/>
        <element label="Workday task created moment" max_length="150" name="workday_created_moment" read_only="true" type="string"/>
        <element label="Workday task ID" max_length="250" name="workday_integration_todo_id" type="string"/>
        <element label="Workday previous assignee ID" max_length="40" name="workday_old_assignee_id" type="string"/>
        <element choice="1" label="Workday task status" max_length="100" name="workday_status" type="string">
            <choice>
                <element label="Awaiting Action" sequence="10" value="awaiting_action"/>
                <element label="In Progress" sequence="20" value="in_progress"/>
                <element label="Saved for Later" sequence="30" value="saved_for_later"/>
                <element label="Approved" sequence="40" value="approved"/>
                <element label="Approvers Added" sequence="50" value="approvers_added"/>
                <element label="Canceled" sequence="60" value="canceled"/>
                <element label="Declined" sequence="70" value="declined"/>
                <element label="Denied" sequence="80" value="denied"/>
                <element label="Manually Advanced" sequence="90" value="manually_advanced"/>
                <element label="Manually Skipped" sequence="100" value="manually_skipped"/>
                <element label="Manually Terminated" sequence="110" value="manually_terminated"/>
                <element label="Manual Send Back" sequence="120" value="manual_send_back"/>
                <element label="Sent Back" sequence="130" value="sent_back"/>
                <element label="Step Completed" sequence="140" value="step_completed"/>
                <element label="Submitted" sequence="150" value="submitted"/>
            </choice>
        </element>
        <element label="Workday current assignee ID" max_length="40" name="workday_user_id" type="string"/>
       <index name="workday_integration_todo_id">
            <element name="workday_integration_todo_id" />
        </index>
    </element>
</database>
